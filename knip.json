{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": ["src/index.ts", "src/commands/**/*.ts", "bin/*.js"],
  "project": ["src/**/*.ts"],
  "ignore": [
    "**/*.spec.ts",
    "**/*.test.ts",
    "dist/**",
    "coverage/**",
    "src/daemon/**"
  ],
  "ignoreBinaries": ["buf"],
  "ignoreDependencies": [
    "tsx",
    "@cspell/dict-he",
    "@types/uuid",
    "ts-proto",
    "@bufbuild/buf"
  ],
  "ignoreExportsUsedInFile": true,
  "ignoreWorkspaces": []
}
